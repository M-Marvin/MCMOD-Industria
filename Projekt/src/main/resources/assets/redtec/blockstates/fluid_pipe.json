{
	"multipart": [
		{ "when": {"middle": "close"}, "apply": {
			"model": "redtec:block/fluid_pipe_mid"
		}},
		{ "when": {"middle": "open"}, "apply": {
			"model": "redtec:block/fluid_pipe_mid_uninsulated"
		}},
		{ "when": {"north": "true"}, "apply": {
			"model": "redtec:block/fluid_pipe_side"
		}},
		{ "when": {"south": "true"}, "apply": {
			"model": "redtec:block/fluid_pipe_side",
			"y": 180
		}},
		{ "when": {"east": "true"}, "apply": {
			"model": "redtec:block/fluid_pipe_side",
			"y": 90
		}},
		{ "when": {"west": "true"}, "apply": {
			"model": "redtec:block/fluid_pipe_side",
			"y": 270
		}},
		{ "when": {"up": "true"}, "apply": {
			"model": "redtec:block/fluid_pipe_up"
		}}
	]
}