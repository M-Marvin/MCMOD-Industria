{
	"multipart": [
		
		{	"when": {"facing": "north", "power": 0 }, 
			"apply": { "model": "industria:block/movement_detector" }
		},
		{	"when": {"facing": "south", "power": 0 }, 
			"apply": { "model": "industria:block/movement_detector", "y": 180 }
		},
		{	"when": {"facing": "east", "power": 0 }, 
			"apply": { "model": "industria:block/movement_detector", "y": 90 }
