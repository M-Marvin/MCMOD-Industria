{
	"multipart": [
		{ "when": {"open": "true"}, "apply": {
			"model": "industria:block/fluid_valve_open"
		}},
		{ "when": {"open": "false"}, "apply": {
			"model": "industria:block/fluid_valve_close"
		}},
		{ "when": {"north": "true"}, "apply": {
			"model": "industria:block/steel_pipe_side"
		}},
		{ "when": {"south": "true"}, "apply": {
			"model": "industria:block/steel_pipe_side",
			"y": 180
		}},
		{ "when": {"east": "true"}, "apply": {
			"model": "industria:block/steel_pipe_side",
			"y": 90
		}},
		{ "when": {"west": "true"}, "apply": {
			"model": "industria:block/steel_pipe_side",
			"y": 270
		}}
	]
}