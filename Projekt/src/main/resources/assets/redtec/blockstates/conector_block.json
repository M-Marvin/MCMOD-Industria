{
    "multipart": [
        {
			"apply": { "model": "redtec:block/conector_block" }
		},
		{   "when": { "north": "true" },
			"apply": { "model": "redtec:block/sticky_conector_block_side" }
		},
		{   "when": { "south": "true" },
			"apply": { "model": "redtec:block/sticky_conector_block_side", "x": 180 }
		},
		{   "when": { "east": "true" },
			"apply": { "model": "redtec:block/sticky_conector_block_side", "y": 90 }
		},
		{   "when": { "west": "true" },
			"apply": { "model": "redtec:block/sticky_conector_block_side", "y": -90 }
		},
		{   "when": { "up": "true" },
			"apply": { "model": "redtec:block/sticky_conector_block_side", "x": -90 }
		},
		{   "when": { "down": "true" },
			"apply": { "model": "redtec:block/sticky_conector_block_side", "x": 90 }
		}
    ]
}