			"model": "redtec:block/conveyor_spliter_base",
			"y": 90
		}},
		{ "when": {"right": "close", "facing": "west"}, "apply": {
			"model": "redtec:block/conveyor_belt_close",
			"y": 270
		}},
		{ "when": {"left": "close", "facing": "west"}, "apply": {
			"model": "redtec:block/conveyor_belt_close",
			"y": 90
		}},
		{ "when": {"right": "open", "facing": "west"}, "apply": {
			"model": "redtec:block/conveyor_spliter_open",
			"y": 270
		}},
		{ "when": {"left": "open", "facing": "west"}, "apply": {
			"model": "redtec:block/conveyor_spliter_open",
			"y": 90
		}},
		{ "when": {"active": "false", "facing": "west", "right": "open"}, "apply": {
			"model": "redtec:block/conveyor_spliter_right",
			"y": 90
		}},
		{ "when": {"active": "true", "facing": "west", "right": "open"}, "apply": {
			"model": "redtec:block/conveyor_spliter_right_active",
			"y": 90
