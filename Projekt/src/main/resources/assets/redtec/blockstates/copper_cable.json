{
	"multipart": [
		{ "when": {"middle_closed": "true"}, "apply": {
			"model": "redtec:block/copper_cable_mid"
		}},
		{ "when": {"middle_closed": "false"}, "apply": {
			"model": "redtec:block/copper_cable_mid_uninsulated"
		}},
		{ "when": {"north": "true"}, "apply": {
			"model": "redtec:block/copper_cable_side"
		}},
		{ "when": {"south": "true"}, "apply": {
			"model": "redtec:block/copper_cable_side",
			"y": 180
		}},
		{ "when": {"east": "true"}, "apply": {
			"model": "redtec:block/copper_cable_side",
			"y": 90
		}},
		{ "when": {"west": "true"}, "apply": {
			"model": "redtec:block/copper_cable_side",
			"y": 270
		}},
		{ "when": {"up": "true"}, "apply": {
			"model": "redtec:block/copper_cable_up"
		}}
	]
}