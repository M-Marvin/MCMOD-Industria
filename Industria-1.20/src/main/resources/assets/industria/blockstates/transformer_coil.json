{
  "multipart": [
    {
      "apply": {
        "model": "industria:block/machines/transformer_core",
        "uvlock": true
      },
      "when": {
        "core": "true"
      }
    },
    {
      "apply": {
        "model": "industria:block/machines/transformer_top",
        "uvlock": true
      },
      "when": {
        "top": "true"
      }
    },
    {
      "apply": {
        "model": "industria:block/machines/transformer_bottom",
        "uvlock": true
      },
      "when": {
        "bottom": "true"
      }
    },
    {
      "apply": {
        "model": "industria:block/machines/transformer_side",
        "uvlock": true
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "industria:block/machines/transformer_side",
        "y": 180,
        "uvlock": true
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "industria:block/machines/transformer_side",
        "y": 90,
        "uvlock": true
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "industria:block/machines/transformer_side",
        "y": -90,
        "uvlock": true
      },
      "when": {
        "west": "true"
      }
    }
  ]
}